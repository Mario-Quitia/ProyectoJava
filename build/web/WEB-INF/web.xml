<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>CrearProductoServlet</servlet-name>
        <servlet-class>Servlet.CrearProductoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GestionProductosServlet</servlet-name>
        <servlet-class>Servlet.GestionProductosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Editarybuscar</servlet-name>
        <servlet-class>Servlet.editarybuscarServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CrearProductoServlet</servlet-name>
        <url-pattern>/CrearProductoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GestionProductosServlet</servlet-name>
        <url-pattern>/GestionProductosServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Editarybuscar</servlet-name>
        <url-pattern>/Editarybuscar</url-pattern>
    </servlet-mapping>
</web-app>
